FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.metal 0
    volScalarFieldValue xi 0.0

);

regions
(


    boxToCell
    {
        box (-0.085 0.0025 -0.5) (0.085 0.0075 0.5);
        fieldValues
        (
    volScalarFieldValue alpha.metal 1
    volScalarFieldValue xi 1.0
        );
    }
    



);


