(


    (-15 0 -15)//0
    (15 0 -15)//1
    (15 30 -15)//2
    (-15 30 -15)//3
    (-15 0 15)//4
    (15 0 15)//5
    (15 30 15)//6
    (-15 30 15)//7



);
