FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.metal 0
    volScalarFieldValue xi 0.0
);

regions
(



     boxToCell
     {
         box (-1 2.501e-3 -1) (1 27.501e-3 1);
         fieldValues
         (
     volScalarFieldValue alpha.metal 1
 volScalarFieldValue xi 1.0
         );
     }


     
     
     
     cylinderToCell
{
p1 	 (0.0 	 0.0 	 0.0);
p2 	 (0.0 	 30.0e-3 	 0.0);
radius 4e-3;
fieldValues
(
     volScalarFieldValue T 1800
 volScalarFieldValue xi 0.0
);
}

     cylinderToCell
{
p1 	 (0.0 	 0.0 	 0.0);
p2 	 (0.0 	 7.501e-3 	 0.0);
radius 6e-3;
fieldValues
(
     volScalarFieldValue T 1800
 volScalarFieldValue xi 0.0
);
}

);


