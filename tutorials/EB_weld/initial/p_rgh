FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
	front{
        type            symmetryPlane;
        value           uniform 0; // does this line need changing/removing?
	}

    back{
        type            fixedFluxPressure;
        value           uniform 0;
	}

    lowerWall
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }

    atmosphere
    {
        type            fixedFluxPressure; 
        value           uniform 0;
        // p0              uniform 0;
        // U               U;
        // phi             phi;
    }

    defaultFaces
    {
        type            empty;
    }
}

