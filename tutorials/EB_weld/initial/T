FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

// Tliq = 1723 1928.15
internalField   uniform 300;

boundaryField
{
	front{
type symmetryPlane;
	}

    back{
        type zeroGradient;
            }

    lowerWall
    {
        type zeroGradient; // changed from fixedValue;
        // value uniform 300.0;
    }

    atmosphere
    {
    type zeroGradient; 
    }

    leftWall{
         type empty;
    }
    rightWall{
         type empty;
    }
}

