FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
	front{
        type symmetryPlane;
        value uniform -1.0;
	}

    back{
        type fixedValue;
        value uniform -1.0;
            }

    lowerWall
    {
        type fixedValue;
        value uniform -1.0;
    }

    atmosphere
    {
        type fixedValue; 
        value uniform 1.0; // need to change?
    }

    defaultFaces
    {
        type            empty;
    }
}

