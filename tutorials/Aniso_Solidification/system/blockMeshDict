vertices//defines the points that bound the mesh
(


    (-0.05 0 0)//0
    (0.05 0 0)//1
    (0.05 1 0)//2
    (-0.05 1 0)//3
    (-0.05 0 2)//4
    (0.05 0 2)//5
    (0.05 1 2)//6
    (-0.05 1 2)//7



);
blocks
(
    hex (0 1 2 3 4 5 6 7) (1 400 800) simpleGrading (1 1 1)		//REGION 1



);

