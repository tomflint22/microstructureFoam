    lowerWall
    {
        type wall;
        faces
        (
            (0 3 2 1)
            

        );
    }

    atmosphere
    {
        type patch;
        faces
        (
            (5 6 7 4)
            
        );
    }
    rightWall
    {
        type empty;
        faces
        (
            (1 2 6 5)
	    
        );
    }
    leftWall
    {
        type empty;
        faces
        (
            (0 4 7 3)
        );
    }
    frontAndBack
    {
        type patch;
        faces
        (
            (3 7 6 2)
            (1 5 4 0)
	    
        );
    }
);

