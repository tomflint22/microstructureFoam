FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.metal 1
    volScalarFieldValue xi 1.0
);

regions
(

     boxToCell
     {
         box (-1 0.0 1e-3) (1 1.0e-3 6.0e-3);
         fieldValues
         (
     volScalarFieldValue T 1724
 volScalarFieldValue xi 0.0
         );
     }

);


