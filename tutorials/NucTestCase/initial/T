FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

// Tliq = 1723 1928.15
internalField   uniform 1724;

boundaryField
{
	frontAndBack{
        type fixedValue;
        value uniform 1720;
	}

    lowerWall
    {
        type fixedValue;
        value uniform 1720;
    }

    atmosphere
    {
        type fixedValue;
        value uniform 1720;
    }

    leftWall{
         type empty;
    }
    rightWall{
         type empty;
    }
}

