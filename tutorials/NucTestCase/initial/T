FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

// Tliq = 1723 1928.15
internalField   uniform 1930;

boundaryField
{

    frontAndBack
    {
        type            empty;
    }

    left
    {
        type fixedValue;
        value uniform 1920;
    }

    bottom
    {
        type fixedValue;
        value uniform 1920;
    }

    right
    {
        type fixedValue;
        value uniform 1920;
    }

    top
    {
        type fixedValue;
        value uniform 1920;
    }
}

